<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine" />
  </configSections>
  <superSocket logFactory="ConsoleLogFactory"
               disablePerformanceDataCollector="true"
               isolation="AppDomain"
               defaultCulture="zh-CN">
    <servers>
      <server name="TestServerA" serverTypeName="TestServer"
              port="2012" maxConnectionNumber="100"
              defaultCulture="zh-TW">
      </server>
      <server name="TestServerB" serverTypeName="TestServer"
              port="2013" maxConnectionNumber="100">
      </server>
    </servers>
    <serverTypes>
      <add name="TestServer"
           type="SuperSocket.Test.TestServer, SuperSocket.Test" />
    </serverTypes>
    <logFactories>
      <add name="ConsoleLogFactory"
           type="SuperSocket.SocketBase.Logging.ConsoleLogFactory, SuperSocket.SocketBase" />
    </logFactories>
  </superSocket>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>
